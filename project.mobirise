          "_name": "form03",
          "_customHTML": "<section class=\"form3\" group=\"Form\" plugins=\"amp-main, amp-form, amp-mustache\">\n    <mbr-parameters>\n        <header>Size</header>\n        <input type=\"range\" inline=\"\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"9\">\n        <input type=\"range\" inline=\"\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"9\">\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" name=\"showsubTitle\" title=\"Show Sub Title\" checked=\"\">\n        <input type=\"checkbox\" name=\"showTitle\" title=\"Show Title\" checked=\"\">\n        <input type=\"checkbox\" name=\"showText1\" title=\"Show Text 1\" checked=\"\">\n        <input type=\"checkbox\" name=\"showText2\" title=\"Show Text 2\">\n        <header>Style</header>\n        <input type=\"color\" title=\"Image Background\" name=\"imgbg\" value=\"#bb4052\" selected=\"\">\n        <input type=\"checkbox\" name=\"customStyle\" title=\"Custom Style\" checked=\"\">\n        <input type=\"checkbox\" name=\"disableElements\" title=\"Disable Control\">\n        <input type=\"color\" name=\"fieldColor\" title=\"Field\" value=\"#b8707c\" condition=\"customStyle\">\n        <input type=\"color\" name=\"fieldHover\" title=\"Field Hover\" value=\"#b8707c\" condition=\"customStyle\">\n        <input type=\"color\" name=\"borderColor\" title=\"Border\" value=\"#ffffff\" condition=\"customStyle\">\n        <input type=\"color\" name=\"borderHover\" title=\"Border Hover\" value=\"#ffffff\" condition=\"customStyle\">\n        <input type=\"color\" name=\"formText\" title=\"Text\" value=\"#ffffff\" condition=\"customStyle\">\n        <input type=\"color\" name=\"formFocusText\" title=\"Text Focus\" value=\"#ffffff\" condition=\"customStyle\">\n        <input type=\"color\" name=\"formPlaceholder\" title=\"Placeholders\" value=\"#efefef\" condition=\"customStyle\">\n        <input type=\"color\" name=\"formFocusPlaceholder\" title=\"Placeholders Focus\" value=\"#efefef\" condition=\"customStyle\">\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\">\n            <input type=\"image\" title=\"Background Image\" value=\"@THEME_PATH@/components/_images/background3.jpg\">\n            <input type=\"color\" title=\"Background Color\" value=\"#ffffff\" selected=\"\">\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked=\"\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#f2e2df\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n        <input type=\"range\" inline=\"\" title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.3\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n        <input type=\"hidden\" name=\"usedRecaptcha\" value=\"false\">\n    </mbr-parameters>\n    <div class=\"mbr-overlay\" mbr-if=\"(overlay &amp;&amp; bg.type=='image')\"></div>\n    <div class=\"container\">\n        <div class=\"mbr-row jc-c-e\">\n\n\n\n\n\n            <div class=\"mbr-col-lg-7 mbr-col-md-12 md-pb mbr-col-sm-12 mbr-white\">\n\n                <h4 class=\"mbr-section-subtitle mbr-fonts-style align-left mbr-semibold mbr-pb-1 mbr-black\" mbr-theme-style=\"display-7\" data-app-selector=\".mbr-section-subtitle\" mbr-if=\"showsubTitle\">get in\n                    touch</h4>\n\n                <h4 mbr-theme-style=\"display-1\" mbr-if=\"showTitle\" class=\"mbr-fonts-style mbr-fonts-style mbr-pb-3 align-left mbr-fonts-style\">Contact Us</h4>\n\n\n                <p mbr-theme-style=\"display-4\" mbr-if=\"showText1\" data-app-selector=\".text1\" class=\"mbr-fonts-style mbr-text text1 align-left mbr-pb-3 mbr-fonts-style\">Lorem ipsum dolor sit\n                    amet, consectetur adipiscing elit. Aliquam malesuada ut magna vel porta. Praesent consectetur\n                    molestie finibus. Fusce vitae urna in quam malesuada rutrum. <br></p>\n\n\n\n\n                <div class=\"form-card\">\n                    <div mbr-form=\"\" class=\"col-lg-12 mx-auto  mbr-form\">\n                        <!--Formbuilder Form-->\n                        <form action=\"\" name=\"Form Name\" method=\"POST\" class=\"mbr-form\">\n                            <div class=\"mbr-row\">\n                                <div submit-success=\"\" class=\"mbr-col-lg-12 mbr-col-md-12 mbr-col-sm-12\">\n                                    <template data-form-alert=\"\" type=\"amp-mustache\">Thanks for filling out the\n                                        form!</template>\n                                </div>\n                                <div submit-error=\"\" class=\"mbr-col-lg-12 mbr-col-md-12 mbr-col-sm-12\">\n                                    <template data-form-alert=\"\" type=\"amp-mustache\">Oops...! some problem!</template>\n                                </div>\n                            </div>\n                            <div class=\"dragArea mbr-row\">\n                                <div data-for=\"form[data][0][1]\" class=\"mbr-col-lg-6 mbr-col-md-12 mbr-col-sm-12 field\">\n                                    <input type=\"hidden\" name=\"form[data][0][0]\" value=\"name\">\n                                    <input type=\"text\" name=\"form[data][0][1]\" placeholder=\"Name\" data-form-field=\"name\" mbr-theme-style=\"display-7\" class=\"field-input\" value=\"\">\n                                </div>\n                                <div data-for=\"form[data][1][1]\" class=\"mbr-col-lg-6 mbr-col-md-12 mbr-col-sm-12 field\">\n                                    <input type=\"hidden\" name=\"form[data][1][0]\" value=\"email\">\n                                    <input type=\"email\" name=\"form[data][1][1]\" placeholder=\"Email\" data-form-field=\"email\" mbr-theme-style=\"display-7\" class=\"field-input\" value=\"\">\n                                </div>\n                      